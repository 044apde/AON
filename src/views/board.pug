extends base.pug
include mixins/list.pug
block content 
    head 
        link(rel="stylesheet" href="/css/board.css")
        title=boardName

    .wrap
        .head
            p.boardTitle #{boardName} 게시판
            a(href=boardName+"/post").linkWriter 글쓰기
        table.boardTable
            each post in posts
                +post(post)

    script(src="/js/board.js") 